# 部署环境配置示例
# 复制此文件为 .env.local 并根据实际环境修改

# API服务器地址
REACT_APP_API_URL=http://57.183.21.242:8080
VITE_API_BASE_URL=http://57.183.21.242:8080

# 部署类型: direct | systemd | ecs | k8s
REACT_APP_DEPLOYMENT_TYPE=direct

# AWS ECS配置（当DEPLOYMENT_TYPE=ecs时使用）
REACT_APP_ECS_CLUSTER=arbitrage-cluster
REACT_APP_AWS_REGION=us-west-2

# Kubernetes配置（当DEPLOYMENT_TYPE=k8s时使用）
REACT_APP_K8S_NAMESPACE=arbitrage
REACT_APP_K8S_CONTEXT=default

# 系统配置
REACT_APP_SYSTEM_NAME=5.1套利系统
REACT_APP_VERSION=5.1.0

# 功能开关
REACT_APP_ENABLE_LOGS=true
REACT_APP_ENABLE_METRICS=true
REACT_APP_ENABLE_CONFIG_MANAGEMENT=true